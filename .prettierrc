{
  "useTabs": false,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 80,
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "htmlWhitespaceSensitivity": "ignore",
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json"
      }
    }
  ],
  "endOfLine": "auto"
}
